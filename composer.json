{
    "name": "renedemat/laravel-bigquery",
    "type": "library",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/renedekat/laravel-bigquery",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Schulze",
            "email": "github@schulze.co",
            "homepage": "https://schulze.co",
            "role": "Developer"
        },
        {
            "name": "Rene de Kat",
            "email": "renedekat@9lives-development.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "google/cloud-bigquery": "^0.3.1",
        "illuminate/support": "~5.4.0|~5.5.0",
        "madewithlove/illuminate-psr-cache-bridge": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.1",
        "mockery/mockery": "^0.9.5",
        "orchestra/testbench" : "~3.4.6|~3.5.0"
    },
    "autoload": {
        "psr-4": {
            "ReneDeKat\\BigQuery\\": "src"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "ReneDeKat\\BigQuery\\BigQueryServiceProvider"
            ],
            "aliases":{
                "BigQuery": "ReneDeKat\\BigQuery\\BigQueryFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReneDeKat\\BigQuery\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
